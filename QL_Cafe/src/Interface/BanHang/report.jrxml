	<queryString language="SQL">
		<![CDATA[SELECT
     chitiethd.`MaChiTietHD` AS chitiethd_MaChiTietHD,
     chitiethd.`SoLuong` AS chitiethd_SoLuong,
     hoadon.`MaBan` AS hoadon_MaBan,
     hoadon.`TongTien` AS hoadon_TongTien,
